# Shortcuts
alias gotest="go test $(go list ./... | grep -v '/vendor/')" # skip vendor dir


# cd
alias ..="cd .."

# ls
alias ls='ls -GFh'
alias ll="ls -lA"

# list aliases

alias listaliases="alias | sed 's/=.*//'"

# Git aliases
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit'
alias gd='git diff'
alias gk='gitk --all&'
alias gx='gitx --all'

alias got='git '
alias get='git '
